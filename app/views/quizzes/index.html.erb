<main class="container">
  <div class="header">
    <h1>Quizzes</h1>
    <%= link_to "New Quiz",new_quiz_path,data: { turbo_frame: dom_id(Quiz.new) },class: "btn btn--primary" %>
  </div>
  <%= turbo_frame_tag Quiz.new %>
  <%= turbo_frame_tag "quizzes" do %>
    <%= render @quizzes %>
  <% end %>
</main>

